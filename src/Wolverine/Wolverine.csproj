    <PackageReference Include="Microsoft.Extensions.Hosting" Version="[6.0.0,7.0.0)" />
    <PackageReference Include="Microsoft.Extensions.ObjectPool" Version="[6.0.0,7.0.0)" />

    <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="[6.0.0,7.0.0)" />
    <PackageReference Include="System.Net.NameResolution" Version="4.3.0" />
    <PackageReference Include="System.Threading.Tasks.Dataflow" Version="6.0.0" />
    <PackageReference Include="System.ComponentModel" Version="4.3.0" />
    <PackageReference Include="Oakton" Version="4.6.1" />
